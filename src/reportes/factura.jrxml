	<queryString>
		<![CDATA[SELECT
     bill2.`nf` AS nf,
     bill2.`fecha` AS fecha,
     bill2.`pedido` AS pedido,
     bill2.`cliente_rnc` AS rnc,
     bill2.`nombre_cliente` AS cliente,
     bill2.`producto` AS producto,
     bill2.`desc_pro` AS desc_pro,
     bill2.`cantidad` AS cant,
     bill2.`precio` AS precio,
     bill2.`total_pedido` AS tpedido,
     bill2.`descuento` AS descu,
     bill2.`total_factura` AS tf,
     bill2.`nombrec` AS nombrec,
     bill2.`nocom` AS nocom
FROM
